{
  "transformIgnorePatterns": [
    "node_modules/(?!(string-width|strip-ansi|ansi-regex|test-json-import)/)"
  ],
  "collectCoverage": true,
  "collectCoverageFrom": [
    "terraform/src/**/*.{js,jsx,ts,tsx}",
    "!terraform/src/tests/**",
    "!**/*.test.{js,jsx,ts,tsx}",
    "!**/*.config.js",
    "!**/node_modules/**"
  ],
  "transform": {
    "^.+\\.[t|j]sx?$": "babel-jest"
  },
  "testEnvironment": "node"
}
