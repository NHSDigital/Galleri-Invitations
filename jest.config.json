{
  "transformIgnorePatterns": [
    "node_modules/(?!(string-width|strip-ansi|ansi-regex|test-json-import|nhs-mesh-client)/)"
  ],
  "collectCoverage": true,
  "collectCoverageFrom": [
    "terraform/src/**/*.{js,jsx,ts,tsx}",
